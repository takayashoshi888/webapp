{
  "compilerOptions": {
    "target": "es6",
    "lib": ["dom", "dom.iterable", "es2018"],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": false,
    "noEmit": true,
    "checkJs": false
  },
  "include": [
    "*.js",
    "**/*.js"
  ],
  "exclude": [
    "node_modules"
  ]
}